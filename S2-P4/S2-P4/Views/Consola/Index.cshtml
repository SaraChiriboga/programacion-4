@model IEnumerable<S2_P4.Models.Consola>
<h2>Catálogo de Consolas</h2>
<table class="table table-striped">
    <thead>
        <tr>
            <th>ID</th>
            <th>Nombre</th>
            <th>Año</th>
            <th>Precio</th>
        </tr>
    </thead>

    <tbody>
        @foreach (var consola in Model)
        {
            <tr>
                <td>@consola.Id</td>
                <td>@consola.Name</td>
                <td>@consola.Ano</td>
                <td>@consola.Precio</td>
            </tr>
        }
    </tbody>
</table>
@{
}
